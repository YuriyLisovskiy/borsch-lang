текст = "Якийсь text.";

@"формули.борщ" -> формули

функція точка_входу(число: цілий, список_продуктів: ...абиякий) -> цілий
{
    друкр("Список продуктів:", список_продуктів);
    якщо(число == 256)
    {
        повернути 1;
    }

    повернути 0;
}
