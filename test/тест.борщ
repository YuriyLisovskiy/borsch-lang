вхідне_число = 11;

10; 10;

ф = 5;
друкр(6.0 - --дійсне(ф));

зовнішня_змінна = 10;

// якщо((вхідне_число >= 5) і (вхідне_число <= 10))
якщо(вхідне_число > 10)
{
    друкр("Якщо");
}
інакше якщо(вхідне_число - 5)
{
    зовнішня_змінна = істина;
    друкр("зовнішня_змінна =", зовнішня_змінна);
}
інакше
{
    друкр("Інакше");
}

друкр("зовнішня_змінна =", зовнішня_змінна);
