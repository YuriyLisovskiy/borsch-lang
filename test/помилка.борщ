підтвердити(1+3*(2-4)*6+2, -33);
підтвердити(-1+3*(2-4)*6+2, -35);
підтвердити(-1+3*(-цілий(2.7)-4)*6+2, -107);

підтвердити(2 + 3 > 4 && істина, істина);
підтвердити(2 + -3 > 4 && істина, хиба);
підтвердити(!2 + -цілий(3.7) > 4 && істина, істина);

