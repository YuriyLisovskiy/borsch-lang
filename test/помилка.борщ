функція факторіал(число: цілий) -> цілий
{
    якщо(число > 1)
    {
        повернути число * факторіал(число - 1);
    }

    повернути 1;
}

підтвердити(факторіал(20), 2432902008176640000, "");

//вихід(тестовий_пакет.точка_входу(256, 2.6, "hello"));

функція ф(жепа: ...цілий?) -> список {
    якщо(жепа != нуль)
    {
        повернути жепа;
    }

    повернути жепа;
}

пес = нуль;

ф(10);
ф(пес);
