ф = 1, 2, "3\"";
переконатися(ф == кортеж(1, 2, "3\""), "розпакування не вдалося, " + рядок(ф) + " != " + рядок(кортеж(1, 2, "3\"")));

а, б = 1, 2;
переконатися(а == 1, "розпакування не вдалося, " + рядок(а) + " != " + рядок(1));
переконатися(б == 2, "розпакування не вдалося, " + рядок(б) + " != " + рядок(2));

б, а = а, б;
переконатися(а == 2, "розпакування не вдалося, " + рядок(а) + " != " + рядок(2));
переконатися(б == 1, "розпакування не вдалося, " + рядок(б) + " != " + рядок(1));

к, л = -5.1, 6;
переконатися(к == -5.1, "розпакування не вдалося, " + рядок(к) + " != " + рядок(-5.1));
переконатися(л == 6, "розпакування не вдалося, " + рядок(л) + " != " + рядок(6));

м, н, о = [7, "8", 2.2];
переконатися(м == 7, "розпакування не вдалося, " + рядок(м) + " != " + рядок(7));
переконатися(н == "8", "розпакування не вдалося, " + рядок(н) + " != " + "8");
переконатися(о == 2.2, "розпакування не вдалося, " + рядок(о) + " != " + рядок(2.2));

п, р, с = ф;
переконатися(п == ф[0], "розпакування не вдалося, " + рядок(п) + " != " + рядок(ф[0]));
переконатися(р == ф[1], "розпакування не вдалося, " + рядок(р) + " != " + рядок(ф[1]));
переконатися(с == ф[2], "розпакування не вдалося, " + рядок(с) + " != " + рядок(ф[2]));
