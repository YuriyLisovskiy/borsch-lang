//а: ціле = 10.7;
//а: логічне = 0;

//друкр(а);
//друкр(б);

клас А
    в = істина;

    функція __конструктор__(я: А)
        я.б = 777;
        я.г = А.в;
    кінець;

    функція ааааа(я: А)
        друкр(я.б + 11111);
    кінець;
кінець;

a = А();

друкр(a);
друкр(a);
друкр(a.б);
друкр(a.в);
а.в = хиба;
друкр(a.в);
друкр(А.в);

а.ааааа();
