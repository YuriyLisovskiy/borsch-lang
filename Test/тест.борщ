/*
@'колекції/стек.борщ' -> стек

ст = стек.Стек();

друкр(ст);

ст.додати(1);
ст.додати("рпе5п678ш№№");
ст.додати(-3.7);

друкр("Розмір: ", ст.розмір());
друкр(ст);

друкр(ст.вилучити());

друкр("Розмір: ", ст.розмір());
друкр(ст);
друкр(ст.вершина());
*/

функція стеля(х: дійсний) -> цілий
{
    _х = дійсний(цілий(х));
    якщо(_х < х)
    {
        _х = _х + 1.0;
    }

    повернути цілий(_х);
}

ст = стеля(1.7);
друкр(ст);

ст = стеля(1.0);
друкр(ст);

ст = стеля(-1.7);
друкр(ст);

друкр(2 + 2 ** 2 * 2);
