текст = "Якийсь text.";

@"формули.борщ" -> формули

фн точка_входу(число: цілий?, список_продуктів: ...абиякий) -> цілий?
{
    друкр("Список продуктів:", список_продуктів);
    якщо(число == 256)
    {
        повернути 1;
    }

    повернути 0;
}

фн отримати_текст() -> рядок
{
    повернути "текст з пакету 'тест'";
}

друкр(логічний + логічний);

