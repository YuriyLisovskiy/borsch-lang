клас Тварина {

    функція __конструктор__(я: Тварина, маса: дійсний) {
        я.маса = маса;
    }
}

клас Кіт : Тварина {

    функція __конструктор__(я: Кіт, маса: дійсний, ім_я: рядок) {
        Тварина.__конструктор__(я, маса);
        я.ім_я = ім_я;
    }
}

друкр(Кіт(5.7, "Яблуко"));
друкр(Кіт(5.7, "Яблуко"));
друкр();
