спс = [1, 2, 3.4, 5.56, "789"];
друкр("Спс:", спс);

а, б, в, г = 1, 2, 3, 4;

друкр("Букви:", а, б, в, г);

а1, а2 = 1, 2;
друкр("А1, А2:", а1, а2);

а2, а1 = а1, а2;
друкр("А1, А2:", а1, а2);

фції = пакет("!/математика/функції.борщ");
друкр(фції);

стеля = фції.стеля;
друкр(фції.стеля);
друкр(стеля);

друкр(стеля(-1.1));

фції.ппп = стеля;
друкр(фції.ппп);

фції.ппп.щщщ = 777;
друкр(фції.ппп.__атрибути__);
//друкр(тип(фції.ппп(7.8)).__атрибути__);

функція яя() {}
функція хх() -> довільний { повернути яя; }

фції.хх = хх;
друкр(фції.хх().__атрибути__);
фції.хх().аа = 1;
//фції.хх() = 1;
друкр(фції.хх().__атрибути__);
