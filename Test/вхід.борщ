блок
    1 / 0.0;
піймати (п: Помилка)
    друкр(п);
    друкр(тип(п));
    друкр(п.__рядок__);
    друкр(п.__рядок__());
    друкр(п.__представлення__());
    друкр(Помилка.__рядок__(п));
кінець;


клас Кіт
    функція __конструктор__(я: Кіт, псєчий: логічне)
        я._псєчий = псєчий;
    кінець;

    функція __рядок__(я: Кіт): ціле
        якщо (я._псєчий)
            повернути "Псєчий кіт!";
        кінець;

        повернути "Файний кіт :)";
    кінець;

//    оператор &&(я: Кіт, інший: Кіт): Кіт
//        повернути Кіт(я._псєчий && інший._псєчий);
//    кінець;

кінець;

