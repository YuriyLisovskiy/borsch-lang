/*
функція заокруглити(х: дійсне, н_знаків: ціле): дійсне
    переконатися(н_знаків > 0, "кількість знаків менша або дорівнює нулю");
    степінь = 10 ** н_знаків;
    х = х * степінь;
    якщо (х >= 0)
        повернути ціле(х + 0.5) / степінь;
    кінець;

    f;
    повернути ціле(х - 0.5) / степінь;
кінець;

функція мейн()
    з = -12344.4545;
    друкр(рядок(з) + " == " + рядок(
        заокруглити(з, 3)
    ));
кінець;

небезпечно
    мейн();
    //1 / 0.0;
піймати(помилка: Помилка)
    друкр("Перехопив: тип " + рядок(тип(помилка)));
    панікувати помилка;
    друкр("Тест Тест Тест");
кінець;
*/

клас А
    функція м(я: А): ціле
        повернути 1;
    кінець;

    лямб = лямбда(х: дійсне): дійсне
        повернути х ** х;
    кінець;
кінець;

а = А();

аФ = а.м;
ааФ = А.м;

друкр(а.м());
друкр(аФ());
друкр(А.м(а));
друкр(ааФ(а));

аЛ = а.лямб;
ааЛ = А.лямб;

друкр(а.лямб(2.0));
друкр(А.лямб(2.0));
друкр(аЛ(2.0));
друкр(ааЛ(2.0));
