
__експортовані__ = ["ВнутрішняПомилка"];

клас ВнутрішняПомилка : Помилка

    // Цей конструктор створений для прикладу ініціалізації обʼєкта
    // атрибутами з базового класу. У даному випадку він є необовʼязковим,
    // оскільки тут відбувається лише виклик конструктора з батьківського класу,
    // що виконується за замовчуванням.
    оператор __конструктор__(я: ВнутрішняПомилка, повідомлення: рядок)
        Помилка.__конструктор__(я, повідомлення);
	кінець;

кінець;
