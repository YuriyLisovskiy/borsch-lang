
__експортовані__ = ["ВнутрішняПомилка"];

клас ВнутрішняПомилка {

	функція __конструктор__(я: ВнутрішняПомилка, повідомлення: рядок) {
		я._повідомлення = повідомлення;
	}

	функція __рядок__(я: ВнутрішняПомилка): рядок {
		повернути я.повідомлення();
	}

	функція __оператор_нерівності__(я: ВнутрішняПомилка, інший: довільний?): логічний {
		якщо (тип(інший) == ВнутрішняПомилка) {
			повернути хиба;
		}

		повернути істина;
	}

	функція повідомлення(я: ВнутрішняПомилка): рядок {
	    повернути я._повідомлення;
	}
}
